{
  "name": "sfv-cli",
  "version": "0.8.2",
  "description": "CLI tool to verify and create SFV files",
  "license": "MIT",
  "scripts": {
    "build": "rollup --config",
    "dev": "npm run start",
    "lint:md": "remark . --quiet --frail --ignore-path .gitignore",
    "lint:ts": "eslint ./src --ignore-path .gitignore",
    "lint": "npm-run-all --parallel lint:*",
    "start": "rollup --watch --config",
    "test": "ava ./test/*.js --verbose"
  },
  "main": "index.js",
  "files": [
    "bin/",
    "README.md"
  ],
  "typings": "./types",
  "engines": {
    "node": ">=10"
  },
  "bin": {
    "sfv": "index.js",
    "node-sfv": "index.js"
  },
  "homepage": "https://www.npmjs.com/package/sfv-cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/node-sfv-cli.git"
  },
  "keywords": [
    "simple file verification",
    "sfv",
    "sfvx",
    "crc",
    "crc32",
    "md5",
    "sha1",
    "sha256",
    "sha512"
  ],
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^6.2.1",
    "globby": "^11.1.0",
    "ora": "^5.4.1",
    "simple-file-verification": "^1.1.0",
    "terminal-link": "^2.1.1",
    "update-notifier": "^5.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^18.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/node": "^14.18.12",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "ava": "^3.15.0",
    "eslint": "^7.32.0",
    "eslint-plugin-json": "^2.1.2",
    "esm": "^3.2.25",
    "execa": "^5.1.1",
    "glob": "^7.2.0",
    "hasha": "^5.2.2",
    "husky": "^4.3.8",
    "jsonlint": "^1.6.3",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "remark-cli": "^9.0.0",
    "remark-preset-lint-recommended": "^5.0.0",
    "remark-preset-prettier": "^0.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.68.0",
    "rollup-plugin-filesize": "^9.1.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "which": "^2.0.2"
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "lint-staged": {
    "*.(json|ts)": "eslint --cache --fix",
    "*.md": "prettier --write"
  }
}
