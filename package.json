{
  "name": "sfv-cli",
  "version": "0.6.1",
  "description": "CLI tool to verify and create SFV files",
  "license": "MIT",
  "scripts": {
    "build": "rollup --config",
    "dev": "npm run start",
    "fix": "eslint --fix ./src",
    "lint": "eslint ./src",
    "start": "rollup --watch --config",
    "test": "ava ./test/*.js --verbose"
  },
  "main": "index.js",
  "typings": "./types",
  "bin": {
    "sfv": "./index.js",
    "node-sfv": "./index.js"
  },
  "homepage": "https://www.npmjs.com/package/sfv-cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/node-sfv-cli.git"
  },
  "keywords": [
    "simple file verification",
    "sfv",
    "sfvx",
    "crc",
    "crc32",
    "md5",
    "sha1",
    "sha256",
    "sha512"
  ],
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^6.0.0",
    "globby": "^11.0.1",
    "ora": "^5.0.0",
    "simple-file-verification": "^1.1.0",
    "terminal-link": "^2.1.1",
    "update-notifier": "^4.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "ava": "^3.11.1",
    "eslint": "^7.7.0",
    "esm": "^3.2.25",
    "execa": "^4.0.3",
    "glob": "^7.1.6",
    "hasha": "^5.2.0",
    "husky": "^4.2.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.4",
    "typescript": ">=3.3.1 <3.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "ava": {
    "require": [
      "esm"
    ]
  }
}
